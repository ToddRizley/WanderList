<% provide(:title, "Log in") %>
  <h1>Log in</h1>
  <%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= submit_tag 'Log In' %>
  <p><%= flash[:notice] %></p>
  <p>New user? <%= link_to "Sign up now!", new_user_path %></p>
<% end %>